<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<Pane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.application.saejeu.saejeu1.controleur.WinControleur">
    <ImageView fitHeight="800.0" fitWidth="1440.0" pickOnBounds="true" preserveRatio="true">
        <image>
            <Image url="@fond-win.jpg" />
        </image>
    </ImageView>


    <VBox alignment="CENTER" layoutX="406.0" layoutY="324.0" prefHeight="199.0" prefWidth="591.0" spacing="50.0" />
    <Button layoutX="673.0" layoutY="536.0" onAction="#retourMenu" style="-fx-font-size: 20px; -fx-text-fill: white; -fx-background-color: red;" text="Rejouer">
        <effect>
            <DropShadow color="black" radius="5.0" spread="0.2" />
        </effect>
    </Button>
    <Button layoutX="675.0" layoutY="592.0" onAction="#quitterApplication" style="-fx-font-size: 20px; -fx-text-fill: white; -fx-background-color: red;" text="Quitter">
        <effect>
            <DropShadow color="black" radius="5.0" spread="0.2" />
        </effect>
    </Button>
</Pane>
